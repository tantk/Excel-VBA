Sub SetWorkbookAttributes()
Dim GetActiveSheet As String, wks As Worksheet

   For Each wks In ActiveWorkbook.Worksheets


  GetActiveSheet = wks.Name
Sheets(GetActiveSheet).Select
ActiveSheet.DrawingObjects.Select
    Selection.ShapeRange.LockAspectRatio = msoFalse
    Selection.ShapeRange.IncrementLeft 0.75
    Selection.ShapeRange.IncrementTop 0.00007874015748
    Selection.ShapeRange.ScaleWidth 0.9993043643, msoFalse, msoScaleFromTopLeft
    Selection.ShapeRange.ScaleHeight 0.6535699935, msoFalse, msoScaleFromTopLeft
    Selection.ShapeRange.PictureFormat.Crop.PictureWidth = 1078
    Selection.ShapeRange.PictureFormat.Crop.PictureHeight = 644
    Selection.ShapeRange.PictureFormat.Crop.PictureOffsetX = 0
    Selection.ShapeRange.PictureFormat.Crop.PictureOffsetY = 111

    Selection.ShapeRange.LockAspectRatio = msoFalse
    Selection.ShapeRange.ScaleWidth 0.7044734241, msoFalse, msoScaleFromTopLeft
    Selection.ShapeRange.ScaleHeight 1.0017791726, msoFalse, msoScaleFromTopLeft
    Selection.ShapeRange.PictureFormat.Crop.PictureWidth = 1078
    Selection.ShapeRange.PictureFormat.Crop.PictureHeight = 644
    Selection.ShapeRange.PictureFormat.Crop.PictureOffsetX = 158
    Selection.ShapeRange.PictureFormat.Crop.PictureOffsetY = 111
   Next wks
End Sub
